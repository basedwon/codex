{
  "name": "@basd/codex",
  "version": "0.0.3",
  "description": "A flexible and robust package that provides a system for managing models, fields, and their validations.",
  "author": "Basedwon <basedwon@protonmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basedwon/codex.git"
  },
  "bugs": {
    "url": "https://github.com/basedwon/codex/issues"
  },
  "homepage": "https://github.com/basedwon/codex#readme",
  "main": "lib/codex.js",
  "browser": {
    ".": "./lib/codex.js"
  },
  "scripts": {
    "dev:test": "nodemon -w ../.. -x npm test",
    "dev": "nodemon -w ../.. test/codex.test.js",
    "docs": "jsdoc2md lib/*.js > docs/api.md",
    "update": "npm run update:prod && npm run update:dev",
    "update:prod": "npm i -S basd@latest @basd/registry@latest",
    "update:dev": "npm i -D @basd/testr@latest",
    "test": "testr test"
  },
  "dependencies": {
    "@basd/registry": "^0.0.1",
    "basd": "^1.0.13"
  },
  "devDependencies": {
    "@basd/testr": "^0.1.2",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0"
  },
  "keywords": [
    "codex",
    "data-modeling",
    "field-validation",
    "schema-parser",
    "registry",
    "model-factory",
    "object-mapping",
    "javascript-orm",
    "data-integration",
    "data-serialization",
    "object-relations",
    "entity-builder",
    "database-abstraction",
    "dynamic-models",
    "model-definitions",
    "type-validation",
    "field-types",
    "data-structure",
    "model-relationships",
    "schema-builder"
  ]
}
